// f(x) = x * x
// f'(x) = 2x
// x_n+1 = x_n - f(x_n) / f'(x_n)
//       = x_n - (x_n * x_n) / (2 * x_n)

var x0 = 0.5;

{
    var x1 = x0 - (x0 * x0) / (2 * x0);
    var x2 = x1 - (x1 * x1) / (2 * x1);
    var x3 = x2 - (x2 * x2) / (2 * x2);
    var x4 = x3 - (x3 * x3) / (2 * x3);

    print "x0: " + x0;
    print "x1: " + x1;
    print "x2: " + x2;
    print "x3: " + x3;
    print "x4: " + x4;

    x4 = x3 + x2 + x1;
    print "x1 + x2 + x3: " + x4;

    if(2 > 4) {
        print "true";
    } else {
        print "false";
    }
}

var control1 = 1;
var control2 = 1;

while (control1 < 100) {
    print "Control 1: " + control1 + ", Control 2: " + control2;

    var previousControl1 = control1;
    control1 = control2;
    control2 = previousControl1 + control2;
}
