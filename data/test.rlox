var count = 0;

fun fib(n) {
    count = count + 1;

    if (n == 1) {
        return 1;
    }
    if (n == 2) {
        return 1;
    }

    return fib(n - 2) + fib(n - 1);
}

var start = clock();

print "25th fibonacci number: " + fib(25);
print "Number of iterations: " + count;

var time = clock() - start;

print "Time to calculate: " + time;
